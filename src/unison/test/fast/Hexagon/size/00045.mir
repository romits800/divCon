--- |
  ; ModuleID = '00045.ll'
  source_filename = "./tests/single-exec/00045.c"
  target datalayout = "e-m:e-p:32:32:32-a:0-n16:32-i64:64:64-i32:32:32-i16:16:16-i1:8:8-f32:32:32-f64:64:64-v32:32:32-v64:64:64-v512:512:512-v1024:1024:1024-v2048:2048:2048"
  target triple = "hexagon"
  
  @x = dso_local global i32 5, align 4
  @y = dso_local local_unnamed_addr global i32 6, align 4
  @p = dso_local local_unnamed_addr global i32* @x, align 4
  
  ; Function Attrs: norecurse nounwind readonly
  define dso_local i32 @main() local_unnamed_addr #0 {
  entry:
    %0 = load i32, i32* @x, align 4, !tbaa !2
    %cmp = icmp eq i32 %0, 5
    br i1 %cmp, label %if.end, label %return
  
  if.end:                                           ; preds = %entry
    %1 = load i32, i32* @y, align 4, !tbaa !6
    %cmp1 = icmp eq i32 %1, 6
    br i1 %cmp1, label %if.end3, label %return
  
  if.end3:                                          ; preds = %if.end
    %2 = load i32*, i32** @p, align 4, !tbaa !8
    %3 = load i32, i32* %2, align 4, !tbaa !2
    %cmp4 = icmp eq i32 %3, 5
    %. = select i1 %cmp4, i32 0, i32 3
    br label %return
  
  return:                                           ; preds = %if.end3, %if.end, %entry
    %retval.0 = phi i32 [ 1, %entry ], [ 2, %if.end ], [ %., %if.end3 ]
    ret i32 %retval.0
  }
  
  attributes #0 = { norecurse nounwind readonly "correctly-rounded-divide-sqrt-fp-math"="false" "disable-tail-calls"="false" "less-precise-fpmad"="false" "min-legal-vector-width"="0" "no-frame-pointer-elim"="true" "no-frame-pointer-elim-non-leaf" "no-infs-fp-math"="false" "no-jump-tables"="false" "no-nans-fp-math"="false" "no-signed-zeros-fp-math"="false" "no-trapping-math"="false" "stack-protector-buffer-size"="8" "target-cpu"="hexagonv5" "target-features"="-long-calls" "unsafe-fp-math"="false" "use-soft-float"="false" }
  
  !llvm.module.flags = !{!0}
  !llvm.ident = !{!1}
  
  !0 = !{i32 1, !"wchar_size", i32 4}
  !1 = !{!"clang version 9.0.0 (http://llvm.org/git/clang.git 4e215e73ed6b499f3ccf23263c96cd188e0eda2c) (git@github.com:unison-code/llvm.git 9914ec00db7eeaf764045b9071638c2421cd32e7)"}
  !2 = !{!3, !3, i64 0}
  !3 = !{!"int", !4, i64 0}
  !4 = !{!"omnipotent char", !5, i64 0}
  !5 = !{!"Simple C/C++ TBAA"}
  !6 = !{!7, !7, i64 0}
  !7 = !{!"long", !4, i64 0}
  !8 = !{!9, !9, i64 0}
  !9 = !{!"any pointer", !4, i64 0}

...
---
name:            main
alignment:       4
exposesReturnsTwice: false
legalized:       false
regBankSelected: false
selected:        false
failedISel:      false
tracksRegLiveness: true
hasWinCFI:       false
registers:       
  - { id: 0, class: intregs, preferred-register: '' }
  - { id: 1, class: intregs, preferred-register: '' }
  - { id: 2, class: intregs, preferred-register: '' }
  - { id: 3, class: intregs, preferred-register: '' }
  - { id: 4, class: predregs, preferred-register: '' }
  - { id: 5, class: predregs, preferred-register: '' }
  - { id: 6, class: intregs, preferred-register: '' }
  - { id: 7, class: intregs, preferred-register: '' }
  - { id: 8, class: predregs, preferred-register: '' }
  - { id: 9, class: predregs, preferred-register: '' }
  - { id: 10, class: intregs, preferred-register: '' }
  - { id: 11, class: intregs, preferred-register: '' }
  - { id: 12, class: predregs, preferred-register: '' }
liveins:         []
frameInfo:       
  isFrameAddressTaken: false
  isReturnAddressTaken: false
  hasStackMap:     false
  hasPatchPoint:   false
  stackSize:       0
  offsetAdjustment: 0
  maxAlignment:    0
  adjustsStack:    false
  hasCalls:        false
  stackProtector:  ''
  maxCallFrameSize: 4294967295
  cvBytesOfCalleeSavedRegisters: 0
  hasOpaqueSPAdjustment: false
  hasVAStart:      false
  hasMustTailInVarArgFunc: false
  localFrameSize:  0
  savePoint:       ''
  restorePoint:    ''
fixedStack:      []
stack:           []
constants:       []
body:             |
  bb.0.entry (freq 32):
    successors: %bb.1(50), %bb.3(50)
  
    ANNOTATION_LABEL <0x8090fe0> = !{!"unison-block-frequency", i64 32}
    %3:intregs = L2_loadrigp @x, <0x809a660> = !{!"unison-memory-partition", i64 0}, implicit $gp :: (dereferenceable load 4 from @x, !tbaa !2)
    %4:predregs = C2_cmpeqi killed %3, 5
    %2:intregs = A2_tfrsi 1
    J2_jumpf killed %4, %bb.3, implicit-def dead $pc
    J2_jump %bb.1, implicit-def dead $pc
  
  bb.1.if.end (freq 16):
    successors: %bb.2(50), %bb.3(50)
  
    ANNOTATION_LABEL <0x809a760> = !{!"unison-block-frequency", i64 16}
    %7:intregs = L2_loadrigp @y, <0x809a660> = !{!"unison-memory-partition", i64 0}, implicit $gp :: (dereferenceable load 4 from @y, !tbaa !6)
    %8:predregs = C2_cmpeqi killed %7, 6
    %6:intregs = A2_tfrsi 2
    J2_jumpf killed %8, %bb.3, implicit-def dead $pc
    J2_jump %bb.2, implicit-def dead $pc
  
  bb.2.if.end3 (freq 8):
    successors: %bb.3(100)
  
    ANNOTATION_LABEL <0x809a8a0> = !{!"unison-block-frequency", i64 8}
    %10:intregs = L2_loadrigp @p, <0x809a660> = !{!"unison-memory-partition", i64 0}, implicit $gp :: (dereferenceable load 4 from @p, !tbaa !8)
    %11:intregs = L2_loadri_io killed %10, 0, <0x809aa60> = !{!"unison-memory-partition", i64 1} :: (load 4 from %ir.2, !tbaa !2)
    %12:predregs = C2_cmpeqi killed %11, 5
    %0:intregs = C2_muxii killed %12, 0, 3
  
  bb.3.return (freq 32):
    liveouts: $pc, $r0
  
    ANNOTATION_LABEL <0x8090fe0> = !{!"unison-block-frequency", i64 32}
    %1:intregs = PHI %2, %bb.0, %6, %bb.1, %0, %bb.2
    $r0 = COPY killed %1
    PS_jmpret $r31, implicit-def dead $pc, implicit killed $r0

...
