--- |
  ; ModuleID = '00149.ll'
  source_filename = "./tests/single-exec/00149.c"
  target datalayout = "e-m:e-p:32:32:32-a:0-n16:32-i64:64:64-i32:32:32-i16:16:16-i1:8:8-f32:32:32-f64:64:64-v32:32:32-v64:64:64-v512:512:512-v1024:1024:1024-v2048:2048:2048"
  target triple = "hexagon"
  
  %struct.S = type { i32, i32 }
  
  @.compoundliteral = internal global %struct.S { i32 1, i32 2 }, align 4
  @s = dso_local local_unnamed_addr global %struct.S* @.compoundliteral, align 4
  
  ; Function Attrs: norecurse nounwind readonly
  define dso_local i32 @main() local_unnamed_addr #0 {
  entry:
    %0 = load %struct.S*, %struct.S** @s, align 4, !tbaa !2
    %cgep5 = bitcast %struct.S* %0 to %struct.S*
    %cgep36 = bitcast %struct.S* %cgep5 to i32*
    %1 = load i32, i32* %cgep36, align 4, !tbaa !6
    %cmp = icmp eq i32 %1, 1
    br i1 %cmp, label %if.end, label %return
  
  if.end:                                           ; preds = %entry
    %2 = bitcast %struct.S* %0 to %struct.S*
    %cgep4 = getelementptr inbounds %struct.S, %struct.S* %2, i32 0, i32 1
    %3 = load i32, i32* %cgep4, align 4, !tbaa !9
    %cmp1 = icmp eq i32 %3, 2
    %. = select i1 %cmp1, i32 0, i32 2
    br label %return
  
  return:                                           ; preds = %if.end, %entry
    %retval.0 = phi i32 [ 1, %entry ], [ %., %if.end ]
    ret i32 %retval.0
  }
  
  attributes #0 = { norecurse nounwind readonly "correctly-rounded-divide-sqrt-fp-math"="false" "disable-tail-calls"="false" "less-precise-fpmad"="false" "min-legal-vector-width"="0" "no-frame-pointer-elim"="true" "no-frame-pointer-elim-non-leaf" "no-infs-fp-math"="false" "no-jump-tables"="false" "no-nans-fp-math"="false" "no-signed-zeros-fp-math"="false" "no-trapping-math"="false" "stack-protector-buffer-size"="8" "target-cpu"="hexagonv5" "target-features"="-long-calls" "unsafe-fp-math"="false" "use-soft-float"="false" }
  
  !llvm.module.flags = !{!0}
  !llvm.ident = !{!1}
  
  !0 = !{i32 1, !"wchar_size", i32 4}
  !1 = !{!"clang version 9.0.0 (http://llvm.org/git/clang.git 4e215e73ed6b499f3ccf23263c96cd188e0eda2c) (git@github.com:unison-code/llvm.git 9914ec00db7eeaf764045b9071638c2421cd32e7)"}
  !2 = !{!3, !3, i64 0}
  !3 = !{!"any pointer", !4, i64 0}
  !4 = !{!"omnipotent char", !5, i64 0}
  !5 = !{!"Simple C/C++ TBAA"}
  !6 = !{!7, !8, i64 0}
  !7 = !{!"S", !8, i64 0, !8, i64 4}
  !8 = !{!"int", !4, i64 0}
  !9 = !{!7, !8, i64 4}

...
---
name:            main
alignment:       4
exposesReturnsTwice: false
legalized:       false
regBankSelected: false
selected:        false
failedISel:      false
tracksRegLiveness: true
hasWinCFI:       false
registers:       
  - { id: 0, class: intregs, preferred-register: '' }
  - { id: 1, class: intregs, preferred-register: '' }
  - { id: 2, class: intregs, preferred-register: '' }
  - { id: 3, class: intregs, preferred-register: '' }
  - { id: 4, class: intregs, preferred-register: '' }
  - { id: 5, class: predregs, preferred-register: '' }
  - { id: 6, class: predregs, preferred-register: '' }
  - { id: 7, class: intregs, preferred-register: '' }
  - { id: 8, class: predregs, preferred-register: '' }
liveins:         []
frameInfo:       
  isFrameAddressTaken: false
  isReturnAddressTaken: false
  hasStackMap:     false
  hasPatchPoint:   false
  stackSize:       0
  offsetAdjustment: 0
  maxAlignment:    0
  adjustsStack:    false
  hasCalls:        false
  stackProtector:  ''
  maxCallFrameSize: 4294967295
  cvBytesOfCalleeSavedRegisters: 0
  hasOpaqueSPAdjustment: false
  hasVAStart:      false
  hasMustTailInVarArgFunc: false
  localFrameSize:  0
  savePoint:       ''
  restorePoint:    ''
fixedStack:      []
stack:           []
constants:       []
body:             |
  bb.0.entry (freq 16):
    successors: %bb.1(50), %bb.2(50)
  
    ANNOTATION_LABEL <0x828d6b0> = !{!"unison-block-frequency", i64 16}
    %0:intregs = L2_loadrigp @s, <0x828d760> = !{!"unison-memory-partition", i64 0}, implicit $gp :: (dereferenceable load 4 from @s, !tbaa !2)
    %4:intregs = L2_loadri_io %0, 0, <0x8294e00> = !{!"unison-memory-partition", i64 1} :: (load 4 from %ir.cgep36, !tbaa !6)
    %5:predregs = C2_cmpeqi killed %4, 1
    %3:intregs = A2_tfrsi 1
    J2_jumpf killed %5, %bb.2, implicit-def dead $pc
    J2_jump %bb.1, implicit-def dead $pc
  
  bb.1.if.end (freq 8):
    successors: %bb.2(100)
  
    ANNOTATION_LABEL <0x8294f00> = !{!"unison-block-frequency", i64 8}
    %7:intregs = L2_loadri_io killed %0, 4, <0x828d760> = !{!"unison-memory-partition", i64 0} :: (load 4 from %ir.cgep4, !tbaa !9)
    %8:predregs = C2_cmpeqi killed %7, 2
    %1:intregs = C2_muxii killed %8, 0, 2
  
  bb.2.return (freq 16):
    liveouts: $pc, $r0
  
    ANNOTATION_LABEL <0x828d6b0> = !{!"unison-block-frequency", i64 16}
    %2:intregs = PHI %3, %bb.0, %1, %bb.1
    $r0 = COPY killed %2
    PS_jmpret $r31, implicit-def dead $pc, implicit killed $r0

...
