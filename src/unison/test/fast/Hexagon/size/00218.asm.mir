  restorePoint:    ''
fixedStack:      []
stack:           []
constants:       []
body:             |
  bb.0.entry (freq 8):
    successors: %bb.1(100)
    liveins: $r0
    liveouts: $p0, $r0, $pc, $r0, $r31
  
    ANNOTATION_LABEL <0x89528c0> = !{!"unison-block-frequency", i64 8}
    renamable $r0 = L2_loadrub_io renamable $r0, 8, <0x892a0b0> = !{!"unison-memory-partition", i64 0} :: (load 1 from %ir.cgep2, align 4)
    BUNDLE implicit-def dead $p0, implicit-def dead $r0, implicit-def $pc, implicit killed $r0, implicit killed $r31 {
      renamable $p0 = C2_cmpeqi killed renamable $r0, 152
      $r0 = C2_cmovenewit internal $p0, 0
      PS_jmprettnew internal killed $p0, killed $r31, implicit-def $pc, implicit-def $pc, implicit internal killed $r0
    }
  
  bb.1.sw.epilog (freq 8):
    liveouts: $pc, $r0
  
    ANNOTATION_LABEL <0x89528c0> = !{!"unison-block-frequency", i64 8}
    BUNDLE implicit-def $r29, implicit-def $r30, implicit-def $r0, implicit-def dead $pc, implicit-def dead $r31, implicit $r29, implicit killed $framekey, implicit killed $framelimit, implicit killed $r30, implicit killed $r31 {
      $r29 = S2_allocframe $r29, 0, implicit-def $r30, implicit killed $framekey, implicit killed $framelimit, implicit killed $r30, implicit killed $r31 :: (store 4 into stack)
      $r0 = A2_tfrsi @str
      J2_call @puts, hexagoncsr, implicit-def dead $pc, implicit-def dead $r31, implicit internal killed $r29, implicit internal killed $r0, implicit-def $r29, implicit-def dead $r0
    }
    BUNDLE implicit-def $d15, implicit-def $r30, implicit-def $r31, implicit-def $r29, implicit-def $r0, implicit killed $r30, implicit killed $framekey {
      $d15 = L2_deallocframe killed $r30, implicit-def $r29, implicit killed $framekey
      $r0 = A2_tfrsi 0
    }
    PS_jmpret killed $r31, implicit-def dead $pc, implicit killed $r0

