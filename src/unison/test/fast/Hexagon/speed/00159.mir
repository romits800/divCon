  entry:
    %call = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([6 x i8], [6 x i8]* @.str, i32 0, i32 0), i32 %a)
    ret void
  }
  
  ; Function Attrs: nounwind
  declare dso_local i32 @printf(i8* nocapture readonly, ...) local_unnamed_addr #2
  
  ; Function Attrs: nounwind
  define dso_local void @qfunc() local_unnamed_addr #1 {
  entry:
    %puts = tail call i32 @puts(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @str, i32 0, i32 0))
    ret void
  }
  
  ; Function Attrs: norecurse noreturn nounwind readnone
  define dso_local void @zfunc() local_unnamed_addr #3 {
  entry:
    unreachable
  }
  
  ; Function Attrs: nounwind
  define dso_local i32 @main() local_unnamed_addr #1 {
  entry:
    %call1 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @.str.2, i32 0, i32 0), i32 9)
    %call3 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([4 x i8], [4 x i8]* @.str.2, i32 0, i32 0), i32 16)
    %call.i = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([6 x i8], [6 x i8]* @.str, i32 0, i32 0), i32 1234) #5
    %puts.i = tail call i32 @puts(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @str, i32 0, i32 0)) #5
    ret i32 0
  }
  
  ; Function Attrs: nounwind
