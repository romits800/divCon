--- |
  ; ModuleID = '00173.ll'
  source_filename = "./tests/single-exec/00173.c"
  target datalayout = "e-m:e-p:32:32:32-a:0-n16:32-i64:64:64-i32:32:32-i16:16:16-i1:8:8-f32:32:32-f64:64:64-v32:32:32-v64:64:64-v512:512:512-v1024:1024:1024-v2048:2048:2048"
  target triple = "hexagon"
  
  @.str = private unnamed_addr constant [6 x i8] c"hello\00", align 1
  @.str.2 = private unnamed_addr constant [8 x i8] c"%c: %d\0A\00", align 1
  @.str.3 = private unnamed_addr constant [21 x i8] c"copied string is %s\0A\00", align 1
  
  ; Function Attrs: nounwind
  define dso_local i32 @main() local_unnamed_addr #0 {
  entry:
    %destarray = alloca [10 x i8], align 8
    %puts = tail call i32 @puts(i8* getelementptr inbounds ([6 x i8], [6 x i8]* @.str, i32 0, i32 0))
    %call6 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @.str.2, i32 0, i32 0), i32 104, i32 104)
    %call6.1 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @.str.2, i32 0, i32 0), i32 101, i32 101)
    %call6.2 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @.str.2, i32 0, i32 0), i32 108, i32 108)
    %call6.3 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @.str.2, i32 0, i32 0), i32 108, i32 108)
    %call6.4 = tail call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([8 x i8], [8 x i8]* @.str.2, i32 0, i32 0), i32 111, i32 111)
    %cgep13 = bitcast [10 x i8]* %destarray to [10 x i8]*
    %cgep114 = bitcast [10 x i8]* %cgep13 to i8*
    call void @llvm.lifetime.start.p0i8(i64 10, i8* nonnull %cgep114) #4
    %cgep715 = bitcast [10 x i8]* %cgep13 to i8*
    store i8 104, i8* %cgep715, align 8, !tbaa !2
    %cgep8 = getelementptr inbounds [10 x i8], [10 x i8]* %cgep13, i32 0, i32 1
    store i8 101, i8* %cgep8, align 1, !tbaa !2
    %cgep9 = getelementptr inbounds [10 x i8], [10 x i8]* %cgep13, i32 0, i32 2
    store i8 108, i8* %cgep9, align 2, !tbaa !2
    %cgep10 = getelementptr inbounds [10 x i8], [10 x i8]* %cgep13, i32 0, i32 3
    store i8 108, i8* %cgep10, align 1, !tbaa !2
    %cgep11 = getelementptr inbounds [10 x i8], [10 x i8]* %cgep13, i32 0, i32 4
    store i8 111, i8* %cgep11, align 4, !tbaa !2
    %cgep12 = getelementptr inbounds [10 x i8], [10 x i8]* %cgep13, i32 0, i32 5
    store i8 0, i8* %cgep12, align 1, !tbaa !2
    %call12 = call i32 (i8*, ...) @printf(i8* getelementptr inbounds ([21 x i8], [21 x i8]* @.str.3, i32 0, i32 0), i8* nonnull %cgep114)
    call void @llvm.lifetime.end.p0i8(i64 10, i8* nonnull %cgep114) #4
    ret i32 0
  }
  
  ; Function Attrs: argmemonly nounwind
  declare void @llvm.lifetime.start.p0i8(i64, i8* nocapture) #1
  
  ; Function Attrs: nounwind
  declare dso_local i32 @printf(i8* nocapture readonly, ...) local_unnamed_addr #2
  
  ; Function Attrs: argmemonly nounwind
  declare void @llvm.lifetime.end.p0i8(i64, i8* nocapture) #1
  
  ; Function Attrs: nounwind
  declare i32 @puts(i8* nocapture readonly) local_unnamed_addr #3
  
  ; Function Attrs: nounwind
  declare void @llvm.stackprotector(i8*, i8**) #4
  
  attributes #0 = { nounwind "correctly-rounded-divide-sqrt-fp-math"="false" "disable-tail-calls"="false" "less-precise-fpmad"="false" "min-legal-vector-width"="0" "no-frame-pointer-elim"="true" "no-frame-pointer-elim-non-leaf" "no-infs-fp-math"="false" "no-jump-tables"="false" "no-nans-fp-math"="false" "no-signed-zeros-fp-math"="false" "no-trapping-math"="false" "stack-protector-buffer-size"="8" "target-cpu"="hexagonv5" "target-features"="-long-calls" "unsafe-fp-math"="false" "use-soft-float"="false" }
  attributes #1 = { argmemonly nounwind "target-cpu"="hexagonv5" }
  attributes #2 = { nounwind "correctly-rounded-divide-sqrt-fp-math"="false" "disable-tail-calls"="false" "less-precise-fpmad"="false" "no-frame-pointer-elim"="true" "no-frame-pointer-elim-non-leaf" "no-infs-fp-math"="false" "no-nans-fp-math"="false" "no-signed-zeros-fp-math"="false" "no-trapping-math"="false" "stack-protector-buffer-size"="8" "target-cpu"="hexagonv5" "target-features"="-long-calls" "unsafe-fp-math"="false" "use-soft-float"="false" }
  attributes #3 = { nounwind "target-cpu"="hexagonv5" }
  attributes #4 = { nounwind }
  
  !llvm.module.flags = !{!0}
  !llvm.ident = !{!1}
  
  !0 = !{i32 1, !"wchar_size", i32 4}
  !1 = !{!"clang version 9.0.0 (http://llvm.org/git/clang.git 4e215e73ed6b499f3ccf23263c96cd188e0eda2c) (git@github.com:unison-code/llvm.git 9914ec00db7eeaf764045b9071638c2421cd32e7)"}
  !2 = !{!3, !3, i64 0}
  !3 = !{!"omnipotent char", !4, i64 0}
  !4 = !{!"Simple C/C++ TBAA"}

...
